{
  "resources": {
    "buckets": {
      "internal": "cumulus-internal",
      "private": "cumulus-internal",
      "public": "cumulus-internal",
      "protected": "cumulus-internal"
    },
    "distribution_endpoint": "https://cumulus.developmentseed.org/distribution/"
  },
  "collection": {
    "name": "CollectionA",
    "granuleIdExtraction": "(.+?)(\\.[^.]*$|$)",
    "meta": {
      "url_path": "testing/cumulus-py",
      "files": [
        {
            "regex": "^input-1.txt$",
            "bucket": "public"
        },
        {
            "regex": "^input-2.txt$",
            "bucket": "private"
        },
        {
            "regex": "^output-1.txt$",
            "bucket": "public"
        },
        {
            "regex": "^output-2.txt$",
            "bucket": "private"
        },
        {
            "regex": "^output-3.cmr.xml$",
            "bucket": "public",
            "url_path": "testing/cumulus-py/otherfolder"
        }]
    }
  },
  "payload": {
    "granules": [
      {
        "granuleId": "TestGranule",
        "files": [
          {"filename": "s3://cumulus-internal/testing/cumulus-py/input-1.txt"},
          {"filename": "s3://cumulus-internal/testing/cumulus-py/input-2.txt"}
        ]
      }
    ]
  }
}

