        },
        {
            "regex": "^output-1.txt$",
            "bucket": "public"
        },
        {
            "regex": "^output-2.txt$",
            "bucket": "private"
        },
        {
            "regex": "^output-3.cmr.xml$",
            "bucket": "public",
            "url_path": "testing/cumulus-py/otherfolder"
        }]
    }
  },
  "input": {
    "granules": [
      {
        "granuleId": "TestGranule",
        "filenames": [
          "s3://cumulus/testing/cumulus-py/input-1.txt",
          "s3://cumulus/testing/cumulus-py/input-2.txt"
