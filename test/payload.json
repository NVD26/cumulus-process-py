    },
    {
        "regex": "^input-2.txt$",
        "bucket": "private"
    },
    {
        "regex": "^output-1.txt$",
        "bucket": "public"
    },
    {
        "regex": "^output-2.txt$",
        "bucket": "private"
    },
    {
        "regex": "^output-3.cmr.xml$",
        "bucket": "public",
        "fileStagingDir": "testing/cumulus-py/otherfolder"
    }],
    "fileStagingDir": "testing/cumulus-py",
    "buckets": {
      "internal": "cumulus",
      "private": "cumulus",
      "public": "cumulus",
      "protected": "cumulus"
    },
    "distribution_endpoint": "https://cumulus..com",
    "input_keys": {
      "input-1": "^.*-1.txt$",
      "input-2": "^.*-2.txt$",
