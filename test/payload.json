{
  "resources": {
    "stack": "cumulus-generic-test",
    "stateMachines": {
      "download": "arn:aws:states:us-west-2:xxxxxxx:stateMachine:cumulus-generic-test-download",
      "process": "arn:aws:states:us-west-2:xxxxxxx:stateMachine:cumulus-generic-test-process",
      "archive": "arn:aws:states:us-west-2:xxxxxxx:stateMachine:cumulus-generic-test-archive",
      "cmr": "arn:aws:states:us-west-2:xxxxxxx:stateMachine:cumulus-generic-test-cmr"
    },
    "buckets": {
      "internal": "cumulus-internal",
      "private": "cumulus-internal",
      "public": "cumulus-internal",
      "protected": "cumulus-internal"
    }
  },
  "collections": {
  	"InputCollection": {
      "granuleId": "",
      "granuleIdExtraction": "",
      "files": {
		"input-1": {
			"regex": "^input-1.txt$",
			"access": "public"
		},
		"input-2": {
			"regex": "^input-2.txt$",
			"access": "private"
		}
      }  		
  	}
  },
  "payload": {
  	"InputCollection": {
  		"files": {
			"input-1": "s3://cumulus-internal/testing/cumulus-py/input-1.txt",
			"input-2": "s3://cumulus-internal/testing/cumulus-py/input-2.txt"
  		}
	}
  }
}
